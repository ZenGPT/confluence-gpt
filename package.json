{
  "name": "gptdock-confluence",
  "version": "0.0.1",
  "private": true,
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --testTimeout=5000",
    "build-jsx-browser": "parcel build --log-level 1 --global __root_component --out-dir views views/*.jsx",
    "build-jsx-node": "parcel build --log-level 1 --no-minify --target node --out-dir views/node views/*.jsx",
    "build": "concurrently -i 'npm:build-*'",
    "start": "node -r esm app.js",
    "watch-server": "nodemon -e js -r esm app.js",
    "watch-jsx-browser": "parcel watch --log-level 1 --no-hmr --global __root_component --out-dir views views/*.jsx",
    "watch-jsx-node": "parcel watch --log-level 1 --target node --out-dir views/node views/*.jsx",
    "watch-jsx": "concurrently 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "watch": "concurrently -i 'npm:watch-server' 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "lint": "eslint app.js routes"
  },
  "dependencies": {
    "@atlaskit/button": "^15.1.0",
    "@atlaskit/checkbox": "^12.4.6",
    "@atlaskit/form": "^8.8.4",
    "@atlaskit/icon": "^21.11.5",
    "@atlaskit/inline-dialog": "^13.4.8",
    "@atlaskit/menu": "^1.5.6",
    "@atlaskit/popup": "^1.5.5",
    "@atlaskit/progress-bar": "^0.5.16",
    "@atlaskit/section-message": "^6.3.9",
    "@atlaskit/textarea": "^4.5.6",
    "@atlaskit/textfield": "^5.3.6",
    "@atlaskit/tooltip": "^17.7.0",
    "atlassian-connect-express": "github:ZenGPT/atlassian-connect-express",
    "axios": "^1.3.4",
    "body-parser": "^1.19.0",
    "clipboard": "^2.0.11",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "errorhandler": "^1.5.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-hbs": "^2.4.0",
    "helmet": "^4.1.1",
    "jest": "^29.5.0",
    "morgan": "^1.10.0",
    "nocache": "^2.1.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^18.2.0",
    "react-markdown": "^8.0.6",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "sequelize": "^6.19.1",
    "styled-components": "^5.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@jest/globals": "^29.5.0",
    "@jest/transform": "^29.5.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.3",
    "longjohn": "^0.2.12",
    "ngrok": "^4.3.3",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.1",
    "sqlite3": "^5.0.0"
  }
}
